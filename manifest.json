{
    "name": "Search Engine Enhancer",
    "description": "Enhances your search engine ;)",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "rick.png"
    }
}